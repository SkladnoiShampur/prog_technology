#include <iostream>
#include <string>

using namespace std;

class Vector {
    
public:
    int a;
public:
    void Create(int test){
        a = test;
    }
    Vector*& operator + (Vector* other){
        Vector* b = new Vector();
        b -> Create(a + other -> a);
        retirn b;
    }
}    
